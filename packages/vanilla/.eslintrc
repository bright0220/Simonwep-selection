{
    "parser": "@typescript-eslint/parser",
    "extends": [
        "../../.eslintrc",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
        "@typescript-eslint"
    ],
    "parserOptions": {
        "project": "./tsconfig.json",
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "rules": {
        "@typescript-eslint/no-non-null-assertion": "off"
    }
}
